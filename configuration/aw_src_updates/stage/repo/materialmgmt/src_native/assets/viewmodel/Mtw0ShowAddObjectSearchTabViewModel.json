{
    "schemaVersion": "1.0.0",
    "imports": 
    [
        "js/aw-search-box.directive",
        "js/aw-tab-set.directive",
        "js/visible-when.directive",
        "js/aw-break.directive",
        "js/aw-panel-section.directive",
        "js/aw-panel-body.directive"
    ],

    "actions": 
    {
        "search": 
        {
            "actionType": "JSFunction",
            "method": "setTypefilterAndSearch",
            "inputData": 
            {
                "inputs": "{{function:setTypefilterAndSearch}}"
            },

            "deps": "js/Mtw0ShowAddObjectService"
        }
    },

  "data": 
    {
        "searchBox": 
        {
            "displayName": "",
            "type": "STRING",
            "isRequired": "true"
        },

        "addObjectSearchTabsModel": 
        {
            "isArray": "true",
            "dbValue": 
            [
                {
                    "tabKey": "results",
                    "panelId": "Mtw0ShowAddObjectSearchResultsTab",
                    "name": "{{i18n.resultsTabTitle}}"
                },

                {
                    "tabKey": "filters",
                    "panelId": "Mtw0ShowAddObjectSearchFilterTab",
                    "name": "{{i18n.filterTabTitle}}"
                }
            ]
        },

        "keyWord": 
        {
            "displayName": "{{i18n.keywordsLabel}}",
            "type": "STRING",
            "isEditable": "false",
            "labelPosition": "PROPERTY_LABEL_AT_TOP",
            "dispValue": "{{ctx.KeyWordsString}}"
        }
    },

    "functions": 
    {
        "setTypefilterAndSearch": 
        {
            "functionName": "setTypefilterAndSearch",
            "parameters": 
            [
                "{{data}}",
                "{{ctx.secondaryObjFromObjectSource}}"
            ]
        }
    },

    "conditions": 
    {
        "showSearchFilterPanel": 
        {
            "expression": "data.showSearchFilter != undefined && data.showSearchFilter == true"
        }
    },

    "i18n": 
    {
        "resultsTabTitle": ["MaterialMgmtMessages"],
        "filterTabTitle": [ "MaterialMgmtMessages"],
        "addPropertiesLabel": ["MaterialMgmtMessages"],
        "keywordsLabel": [ "awAddDirectiveMessages" ]
    }
}
