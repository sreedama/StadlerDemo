      class="com.ebsolutions.uimanager.modules.SaveModule">

    <!-- Columns definition -->
    <Column name="status"/>
    <Column name="catia_status" width="60"/>
    <Column name="save_mode"/>
    <Column name="TC Item item_id" display="Item ID" width="100"/>
    <Column name="TC Item sr5AltRepCounter" display="AltRep" width="50"/>
    <Column name="TC ItemRev item_revision_id" display="Revision" width="50"/>
    <Column name="document_type"/>
    <Column name="TC Item object_type"/>
    <Column name="TC ItemRev object_name" width="100"/>
    <Column name="co_status" width="29"/>
    <Column name="catalog_comp" width="29" visible="false"/>

