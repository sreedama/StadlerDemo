{
    "schemaVersion": "1.0.0",
    "imports": [
        "js/aw-panel.directive",
        "js/aw-panel-body.directive",
        "js/aw-panel-footer.directive",
        "js/aw-chart.directive"
    ],
    "actions": {
        "getChartDataActionForMultipleSeries": {
            "actionType": "JSFunction",
            "method": "createChartFromArrayOfSeries",
            "inputData": {
                 "curveObj": "{{ctx.selected}}"
            },
            "outputData": {
                "chartProviders.chartProvider.chartPoints": ""
            },
            "deps": "js/Mtw0ShowCurve"
        }
    },
    "chartProviders": {
        "chartProvider": {
            "title": "{{ctx.selected.props.object_name.uiValues[0]}}",
            "chartType": "line",
            "loadDataAction": "getChartDataActionForMultipleSeries",
            "chartConfig": {
                "isChartZoomable": true,
                "isYAxisLinearOrLogarithmic": "linear",
                "xAxisLabel": "X",
                "yAxisLabel": "Y",
                "isDataLabelOnChartEnabled": false
            }
        }
    },
    "onEvent": [
        {
            "eventId": "chartProvider.selected",
            "cacheEventData": true
        },
        {
            "eventId": "chartProvider.unselected",
            "cacheEventData": true
        }
    ],
    "messages": {},
    "i18n": {}
}