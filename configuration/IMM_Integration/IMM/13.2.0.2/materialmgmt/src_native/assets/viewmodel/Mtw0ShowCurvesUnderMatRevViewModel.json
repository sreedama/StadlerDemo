        "Mtw0SmartTablesListDataProvider_SelectionChangeEventAction": {
            "actionType": "JSFunction",
            "method": "Mtw0AddRemoveSmartCurvesChart",
            "inputData": {
                "smarttableSelected": "{{data.dataProviders.Mtw0SmartTablesListDataProvider.selectedObjects[0]}}",
                "smartCurveSelectedRow": "{{data.dataProviders.Mtw0SmartCurveTableDataProvider.selectedObjects[0]}}",
                "data": "{{data}}"
            },
            "deps": "js/Mtw0ShowCurvesUnderMatRev"
        },
        "Mtw0InheritedTablesListDataProvider_SelectionChangeEventAction": {
            "actionType": "JSFunction",
            "method": "Mtw0AddRemoveSmartCurvesChart",
            "inputData": {
                "smarttableSelected": "{{data.dataProviders.Mtw0InheritedTablesListDataProvider.selectedObjects[0]}}",
                "smartCurveSelectedRow": "{{data.dataProviders.Mtw0InheritedTablesListDataProvider.selectedObjects[0]}}",
                "data": "{{data}}"
            },
            "deps": "js/Mtw0ShowCurvesUnderMatRev"
        },
        "Mtw0GetSmartCurveTableColumnsAction": {
            "actionType": "JSFunctionAsync",
            "method": "Mtw0GetSmartCurveTableColumns",
            "inputData": {
                "dataProvider": "{{data.dataProviders.Mtw0SmartCurveTableDataProvider}}",
                "xAxisLabel": "{{data.chartProviders.Mtw0SmartCurvesChartProvider.chartConfig.xAxisLabel}}",
                "yAxisLabel": "{{data.chartProviders.Mtw0SmartCurvesChartProvider.chartConfig.yAxisLabel}}"
            },
            "deps": "js/Mtw0ShowCurvesUnderMatRev"
        },
        "Mtw0GetSmartCurveTableDataAction": {
            "actionType": "JSFunction",
            "method": "Mtw0GetSmartCurveTableData",
            "inputData": {
                "data": "{{data}}",
                "Mtw0SelectedSmartCurve": "{{data.eventMap['Mtw0SmartCurvesChartProvider.selected']}}"
            },
            "deps": "js/Mtw0ShowCurvesUnderMatRev"
        }
    },
    "functions": {
        "Mtw0GetSmartTablesFromMaterialRevision": {
            "functionName": "Mtw0GetSmartTablesFromMaterialRevision",
            "parameters": [
                "{{data}}"
            ]
        }
    },
    "onEvent": [
		{
			"eventId": "awPanel.reveal",
			"action": "loadInteritedTables"
		},
		{
			"eventId": "awPanel.reveal",
			"action": "loadSmartTables"
		},
        {
            "eventId": "Mtw0SmartTablesListDataProvider.selectionChangeEvent",
            "action": "Mtw0SmartTablesListDataProvider_SelectionChangeEventAction",
            "cacheEventData": true
        },
        {
            "eventId": "Mtw0InheritedTablesListDataProvider.selectionChangeEvent",
            "action": "Mtw0InheritedTablesListDataProvider_SelectionChangeEventAction",
            "cacheEventData": true
        },
        {
            "eventId": "Mtw0SmartCurvesChartProvider.selected",
            "action": "Mtw0SmartCurvesChartProvider_SelectionEventAction",
            "cacheEventData": true
        },
        {
            "eventId": "Mtw0.SmartCurveTable.StartEdit",
            "action": "Mtw0SmartCurveTable_StartEditsEventAction"
        },
        {
            "eventId": "Mtw0.SmartCurveTable.SaveEdits",
            "action": "Mtw0SmartCurveTable_SaveEditsEventAction"
        },
        {
            "eventId": "Mtw0.SmartCurveTable.CancelEdits",
            "action": "Mtw0SmartCurveTable_CancelEditsEventAction"
        },
        {
            "eventId": "Mtw0.SmartCurveTable.AddRow",
            "action": "Mtw0SmartCurveTable_AddRowEventAction",
            "cacheEventData": true
        },
        {
            "eventId": "Mtw0.SmartCurveTable.DuplicateRows",
