<!-- Default style sheet for displaying EngProject rev summary. -->
<rendering>
    <header>
        <image source="type"/>
        <property name="owning_user"/>
        <property name="last_mod_date"/>
        <property name="object_type"/>
    </header>
    <page titleKey="tc_xrt_Overview" visibleWhen="ActiveWorkspace:SubLocation != com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation and ActiveWorkspace:SubLocation != com.siemens.splm.client.cba.CADBOMAlignment:CBASublocation and {pref:EnterpriseBOM_feature_installed}!=true">
        <column>
            <section titleKey="tc_xrt_properties">
                <property name="item_id" renderingHint="label"/>
                <property name="object_name"/>
                <property name="object_desc"/>
                <property name="object_type"/>
                <separator/>
                <property name="owning_user" renderingHint="objectlink" modifiable="false"/>
                <property name="owning_group" renderingHint="objectlink" modifiable="false"/>
                <property name="last_mod_user"/>
                <property name="checked_out" renderingHint="label"/>
                <property name="checked_out_user"/>
                <separator/>
                <property name="REF(items_tag,SR5EngProject).sr5Customer"/>
                <property name="REF(items_tag,SR5EngProject).REF(sr5Customer,SR5Customer).sr5CustomerNumber"/>
                <property name="REF(items_tag,SR5EngProject).sr5ContractNumber"/>
                <property name="REF(items_tag,SR5EngProject).sr5TypeOfProduct"/>
                <property name="REF(items_tag,SR5EngProject).sr5NumberOfVehicles"/>
                <separator/>
                <property name="REF(items_tag,SR5EngProject).sr5VehicleType"/>
                <property name="REF(items_tag,SR5EngProject).sr5TypeOfPowerSupply"/>
                <property name="REF(items_tag,SR5EngProject).sr5KindOfEngine"/>
                <separator/>
                <property name="REF(items_tag,SR5EngProject).sr5DesigningPlant"/>
                <property name="REF(items_tag,SR5EngProject).sr5ManagingPlant"/>
                <property name="REF(items_tag,SR5EngProject).sr5ProductionPlant"/>
                <separator/>
                <property name="REF(items_tag,SR5EngProject).sr5ProjectManager"/>
                <property name="REF(items_tag,SR5EngProject).sr5ProjectManagerCommercial"/>
                <property name="REF(items_tag,SR5EngProject).sr5ProjectManagerMechanical"/>
                <property name="REF(items_tag,SR5EngProject).sr5ProjectManagerElectrical"/>
            </section>
        </column>
    </page>
    <page titleKey="Coating and Finish">
        <section title="Color Definitions" titleKey="strxrt_colordefinitions" initialstate="expanded">
            <objectSet source="SR5HasProjectColor.SR5ColorDef" sortdirection="ascending" sortby="object_string" defaultdisplay="tableDisplay" showConfiguredRev="true">
                <thumbnailDisplay/>
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </tableDisplay>
                <listDisplay/>
                <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                    <parameter name="localSelection" value="true"/>
                </command>
            </objectSet>
        </section>
        <section title="Coating Structures" titleKey="strxrt_coatingstructures" initialstate="expanded">
            <objectSet source="SR5HasProjectCoatingStruct.SR5CoatingStruct" sortdirection="ascending" sortby="object_string" defaultdisplay="tableDisplay" showConfiguredRev="true">
                <thumbnailDisplay/>
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </tableDisplay>
                <listDisplay/>
                <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                    <parameter name="localSelection" value="true"/>
                </command>
            </objectSet>
        </section>
    </page>
    <page titleKey="Supplier Order Requirements">
        <objectSet source="SR5HasRequiredDocument.SR5GenDocument" sortdirection="ascending" sortby="sr5ReqVerDocumentContent" defaultdisplay="tableDisplay">
            <tableDisplay>
                <property name="object_string"/>
                <property name="sr5ReqVerDocumentContent" modifiable="false"/>
                <property name="SR5HasRequiredDocument.sr5RequiredLanguages" renderingHint="checkboxoptionlov"/>
                <property name="owning_user"/>
            </tableDisplay>
            <thumbnailDisplay/>
            <treeDisplay>
                <property name="object_string"/>
                <property name="sr5ReqVerDocumentContent" modifiable="false"/>
                <property name="SR5HasRequiredDocument.sr5RequiredLanguages" renderingHint="checkboxoptionlov"/>
                <property name="owning_user"/>
            </treeDisplay>
            <listDisplay/>
            <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
            <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
            <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                <parameter name="localSelection" value="true"/>
            </command>
        </objectSet>
    </page>

    <page titleKey="web_whereused">
        <column width="100%">
            <section titleKey="tc_xrt_UsedInStructures">
                <inject type="dataset" src="UsedInStructures"/>
            </section>
            <section titleKey="tc_xrt_References">
                <objectSet source="Awp0ReferencesProvider.WorkspaceObject" sortdirection="ascending" sortby="object_string" defaultdisplay="tableDisplay">
                    <thumbnailDisplay/>
                    <tableDisplay>
                        <property name="object_string"/>
                        <property name="object_type"/>
                        <property name="release_status_list"/>
                        <property name="date_released"/>
                        <property name="owning_user"/>
                    </tableDisplay>
                    <listDisplay/>
                </objectSet>
            </section>
        </column>
    </page>
    <page titleKey="attachments">
        <inject type="dataset" src="SR5SpecificationAndReferenceDocuments"/>
    </page>
    <inject type="dataset" src="Cm1ChangeRelationSummary"/>
    <page titleKey="tc_xrt_History">
        <section title=" ">
            <objectSet source="revision_list.ItemRevision" sortdirection="descending" sortby="last_mod_date" defaultdisplay="tableDisplay">
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </treeDisplay>
                <listDisplay/>
            </objectSet>
        </section>
        <inject type="dataset" src="Cm1ChangeHistorySummary" />
    </page>
    <inject type="dataset" src="WorkflowSummary" />
    <inject type="dataset" src="RelationsSummary" />
</rendering>