<!-- Default style sheet for displaying generic document rev summary. -->
<rendering>
    <header>
        <image source="type"/>
    </header>
    <page titleKey="tc_xrt_Overview" visibleWhen="ActiveWorkspace:SubLocation != com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation">

        <column width="35%">
            <section titleKey="tc_xrt_properties">
                <property name="item_id" renderingHint="label"/>
                <property name="item_revision_id" renderingHint="label"/>
                <property name="sr5ExternalRevision" modifiable="false" />
                <property name="sr5CPBasedOn" />
                <property name="based_on" />
                <property name="object_name"/>
                <property name="object_desc"/>
                <property name="object_type"/>
                <property name="REF(items_tag,SR5GenDocument).uom_tag"/>
                <separator/>
                <property name="sr5Manufacturer" />
                <separator/>
                <property name="sr5DocumentContent"/>
                <property name="sr5DocumentFormat" />
                <property name="sr5NumberOfSheets" />
                <property name="sr5ExpiryDate" />
                <property name="REF(items_tag,SR5GenDocument).sr5ConfidentialityClass" />
                <separator/>
                <property name="release_status_list" renderingHint="label"/>
                <break/>
                <property name="sr5CPReleasedBy" renderingHint="label" />
                <property name="sr5CPDateReleased" renderingHint="label"/>
                <break/>
                <property name="sr5CPReviewedBy" renderingHint="label" />
                <property name="sr5CPDateReviewed" renderingHint="label" />
                <content visibleWhen="sr5IsTransferredToSAP==1">
                    <separator/>
                    <property name="sr5SAPID"/>
                    <property name="sr5SAPTransDate"/>
                    <property name="sr5SAPTransStatus"/>
                </content>
                <separator/>
                <property name="owning_user" renderingHint="objectlink" modifiable="false"/>
                <property name="owning_group" renderingHint="objectlink" modifiable="false"/>
                <property name="last_mod_user"/>
                <property name="checked_out_date" renderingHint="label"/>
                <property name="checked_out_user" renderingHint="label"/>
                <property name="checked_out_date" renderingHint="label"/>
            </section>
            <section title="Coating Specification" titleKey="strxrt_coatingspecification" initialstate="expanded">
                <objectSet source="SR5HasSpecificationDocument.SR5GenDocument" defaultdisplay="listDisplay" showConfiguredRev="true" maxRowCount="" sortby="object_string" sortDirection="ascending" enablePropEdit="false" filterable="true">
                    <tableDisplay enableArrange="true">
                        <property name="object_string" />
                        <property name="object_type" />
                        <property name="release_status_list" />
                        <property name="date_released" />
                        <property name="owning_user" />
                    </tableDisplay>
                    <listDisplay/>
                    <imageDisplay/>
                    <thumbnailDisplay/>
                    <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                    <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                    <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                        <parameter name="localSelection" value="true"/>
                    </command>
                </objectSet>
            </section>
            <section titleKey="tc_xrt_References">
                <objectSet source="IMAN_specification.WorkspaceObject,Awp0ReferencesProvider.WorkspaceObject,REF(items_tag,SR5GenDocument).GRMS2P(SR5HasSpecificationDocument,SR5GenPartRevision),REF(items_tag,SR5GenDocument).GRMS2P(SR5HasRequiredDocument,SR5GenPartRevision),REF(items_tag,SR5GenDocument).GRMS2P(IMAN_reference,SR5GenPartRevision)" sortdirection="ascending" sortby="object_string" defaultdisplay="listDisplay">
                    <tableDisplay>
                        <property name="object_string"/>
                        <property name="object_type"/>
                        <property name="release_status_list"/>
                        <property name="date_released"/>
                        <property name="owning_user"/>
                    </tableDisplay>
                    <listDisplay/>
                    <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                    <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                    <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                        <parameter name="localSelection" value="true"/>
                    </command>
                </objectSet>
            </section>
            <!-- <inject type="dataset" src="Fnd0ClassificationSummary"/> -->
        </column>
        <column width="65%">
            <section titleKey="tc_xrt_Files">
                <objectSet source="TC_Attaches.Dataset,IMAN_manifestation.Dataset" defaultdisplay="tableDisplay" sortby="object_string" sortdirection="ascending">
                    <tableDisplay>
                        <property name="object_string"/>
                        <property name="object_type"/>
                        <property name="checked_out" renderingHint="label"/>
                        <property name="sr5DocLanguage"/>
                        <property name="relation" modifiable="false"/>
                        <property name="release_status_list"/>
                        <property name="date_released"/>
                        <property name="owning_user"/>
                        <property name="last_mod_date"/>
                        <property name="last_mod_user"/>
                        <property name="Fnd0LogicalObjectTypeRel" />
                    </tableDisplay>
                    <thumbnailDisplay/>
                    <treeDisplay>
                        <property name="object_string"/>
                        <property name="object_type"/>
                        <property name="checked_out" renderingHint="label"/>
                        <property name="sr5DocLanguage"/>
                        <property name="release_status_list"/>
                        <property name="date_released"/>
                        <property name="owning_user"/>
                    </treeDisplay>
                    <listDisplay/>
                    <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                    <command commandId="Dma1Delete" />
                    <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                    <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                        <parameter name="localSelection" value="true"/>
                    </command>
                </objectSet>
            </section>
            <section titleKey="tc_xrt_Viewer">
                <inject type="dataset" src="Awp0CrossSelectionViewer" module="js/aw-include.directive" />
            </section>
        </column>
    </page>
    <page titleKey="tc_xrt_History">
        <section titleKey="tc_xrt_RevisionHistory">
            <objectSet source="revision_list.ItemRevision" sortdirection="descending" sortby="last_mod_date" defaultdisplay="tableDisplay">
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </treeDisplay>
                <listDisplay/>
            </objectSet>
        </section>
        <inject type="dataset" src="Cm1ChangeHistoryNonIRSummary" />
    </page>
    <inject type="preference" src="ClassificationStylesheetTab"/>
    <page titleKey="web_whereused" visibleWhen="ps_parents!=null">
        <section titleKey="tc_xrt_ParentAssemblies">
            <objectSet source="ps_parents.ItemRevision" sortdirection="ascending" sortby="object_string" defaultdisplay="listDisplay">
                <command commandId="com.teamcenter.rac.pse.sendto" titleKey="tc_xrt_OpenSelectedInStructureManager" renderingHint="commandbutton">
                    <parameter value="true" name="localSelection"/>
                </command>
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="object_type"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </treeDisplay>
                <listDisplay/>
            </objectSet>
        </section>
    </page>
    <inject type="dataset" src="Cm1ChangeRelationSummary"/>
    <inject type="dataset" src="WorkflowSummary" />
    <inject type="dataset" src="RelationsSummary" />
</rendering>