                    <parameter name="localSelection" value="true"/>
                </command>
            </objectSet>
        </section>
        -->
        <section titleKey="strxrt_specifyingdocuments">
            <objectSet source="IMAN_specification.SR5GenDocumentRevision" sortdirection="ascending" sortby="object_string" defaultdisplay="tableDisplay">
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="sr5DocumentType" modifiable="false"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="sr5DocumentType" modifiable="false"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </treeDisplay>
                <listDisplay/>
                <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                    <parameter name="localSelection" value="true"/>
                </command>
            </objectSet>
        </section>
        <section titleKey="strxrt_referencingdocuments">
            <objectSet source="IMAN_reference.SR5GenDocumentRevision" sortdirection="ascending" sortby="object_string" defaultdisplay="tableDisplay">
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="sr5DocumentType" modifiable="false"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="sr5DocumentType" modifiable="false"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                </treeDisplay>
                <listDisplay/>
                <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                    <parameter name="localSelection" value="true"/>
                </command>
            </objectSet>
        </section>
        <inject type="dataset" src="Awn0ValidationResultsSummary"/>
        <inject type="dataset" src="Cm1PartRevSummaryPrimaryToPrimaryDocRel"/>
    </page>
    <content visibleWhen="structure_revisions==null">
             <inject type="dataset" src="Cm1ChangeRelationSummary"/>
    </content>
    <!--<inject type="dataset" src="Mtw0CompositionTabForItemRevLocationSummary"/>-->
    <inject type="dataset" src="Scw0ComplianceCategoriesSummary"/>
    <inject type="dataset" src="Scw0ComplianceInfoForBOM"/>
    <page titleKey="tc_xrt_History" visibleWhen="structure_revisions==null">
        <section title=" ">
            <objectSet source="revision_list.ItemRevision" sortdirection="descending" sortby="last_mod_date" defaultdisplay="tableDisplay">
                <tableDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </tableDisplay>
                <thumbnailDisplay/>
                <treeDisplay>
                    <property name="object_string"/>
                    <property name="release_status_list"/>
                    <property name="date_released"/>
                    <property name="owning_user"/>
                    <property name="last_mod_date"/>
                    <property name="item_revision_id" hidden="true" />
                    <property name="sr5ExternalRevision" hidden="true" />
                </treeDisplay>
                <listDisplay/>
            </objectSet>
        </section>
        <inject type="dataset" src="Cm1ChangeHistorySummary" />
    </page>
    <inject type="dataset" src="WorkflowSummaryForShowObjectLocation" />
    <inject type="dataset" src="Relations2Summary" />
    <content visibleWhen="structure_revisions==null">
        <inject type="dataset" src="S2clItemRevSummary"/>
    </content>
    <inject type="dataset" src="Cae1ItemRevSummaryForShowObjectLocation"/>
    <subRendering>
        <content visibleWhen="structure_revisions==null">
            <inject type="dataset" src="Awn0ItemRevSummary"/>
        </content>
    </subRendering>
    <content visibleWhen="structure_revisions==null">
        <inject type="dataset" src="Rb0InContextReportsSubLocation" />
    </content>
    <content visibleWhen="structure_revisions==null">
        <inject type="dataset" src="AuditLogForItemRev"/>
    </content>
    <page title="SAP Status" visibleWhen="structure_revisions==null and ActiveWorkspace:SubLocation != com.siemens.splm.client.occmgmt:OccurrenceManagementSubLocation">
        <section>
            <property name="sr5SAPID"/>
            <property name="sr5SAPTransDate"/>
            <property name="sr5SAPTransStatus"/>
        </section>
    </page>
</rendering>