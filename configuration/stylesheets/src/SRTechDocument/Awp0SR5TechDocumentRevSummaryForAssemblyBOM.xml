<?xml version="1.0" encoding="UTF-8"?>
<!-- Default style sheet for displaying Document Revision Overview for intermediate object representation. -->
<rendering>
    <content visibleWhen="awb0UnderlyingObjectType != Bhm0CollectionRevision and awb0UnderlyingObjectType != Bhm0SimModelRepRevision and awb0UnderlyingObjectType != Bhm0BehaviorModlRevision and awb0UnderlyingObjectType != Bhm0SupportingRevision and awb0UnderlyingObjectType != Bhm0ComponentRepRevision">
        <page titleKey="tc_xrt_Overview" visibleWhen="awb0UnderlyingObjectType != Fnd0WorksetRevision">
            <column width="35%">
                <section titleKey="tc_xrt_Properties">
                    <property name="awb0ArchetypeId"/>
                    <property name="awb0ArchetypeRevId"/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5ExternalRevision" modifiable="false" />
                    <property name="REF(awb0UnderlyingObject.SR5GenDocumentRevision).sr5CPBasedOn" />
                    <property name="REF(awb0UnderlyingObject.SR5GenDocumentRevision).based_on" />
                    <property name="awb0ArchetypeRevName"/>
                    <property name="awb0ArchetypeRevDescription"/>
                    <property name="awb0UnderlyingObjectType" />
                    <property name="awb0Sequence"/>
                    <content visibleWhen="awb0Parent==null and awb0UnderlyingObjectType==ItemRevision and REF(awb0UnderlyingObject,ItemRevision).awb0IsDiscoveryIndexed==true">
                        <property name="REF(awb0UnderlyingObject,ItemRevision).awb0IsDiscoveryIndexed" renderingHint="label"/>
                        <property name="REF(awb0UnderlyingObject,ItemRevision).awb0DiscoveryIndexTime" renderingHint="label"/>
                    </content>
                    <property name="awb0Quantity"/>
                    <property name="awb0UoM" renderingHint="lovcombobox"/>
                    <separator/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5DocumentContent"/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5DocumentFormat" />
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5NumberOfSheets" />
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5ExpiryDate" />
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).REF(items_tag,SR5GenDocument).sr5ConfidentialityClass" />
                    <separator/>
                    <property name="awb0ArchetypeRevRelStatus" renderingHint="label"/>
                    <break/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5CPReleasedBy" renderingHint="label" />
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5CPDateReleased" renderingHint="label"/>
                    <break/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5CPReviewedBy" renderingHint="label" />
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5CPDateReviewed" renderingHint="label" />
                    <content visibleWhen="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5IsTransferredToSAP==1">
                        <separator/>
                        <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5SAPID"/>
                        <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5SAPTransDate"/>
                        <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).sr5SAPTransStatus"/>
                    </content>
                    <separator/>
                    <property name="awb0ArchetypeRevOwningUser" renderingHint="objectlink" modifiable="false"/>
                    <property name="awb0ArchetypeRevOwningGroup" renderingHint="objectlink" modifiable="false"/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).creation_date" renderingHint="label"/>
                    <property name="awb0ArchetypeRevLastModDate" renderingHint="objectlink"/>
                    <property name="awb0ArchetypeRevLastModUser" renderingHint="objectlink"/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).checked_out_user" renderingHint="objectlink"/>
                    <property name="REF(awb0UnderlyingObject,SR5GenDocumentRevision).checked_out_date" renderingHint="label"/>
                </section>
            </column>
            <content  visibleWhen ="awb0UnderlyingObjectType != null">
                <column width="65%">
                    <section titleKey="tc_xrt_Files">
                        <objectSet source="REF(awb0UnderlyingObject,SR5GenDocumentRevision).GRM(TC_Attaches,Dataset),REF(awb0UnderlyingObject,SR5GenDocumentRevision).GRM(IMAN_manifestation,Dataset)" defaultdisplay="tableDisplay" sortby="object_string" sortdirection="ascending">
                            <tableDisplay>
                                <property name="object_string"/>
                                <property name="object_type"/>
                                <property name="checked_out" renderingHint="label"/>
                                <property name="sr5DocLanguage"/>
                                <property name="relation" modifiable="false"/>
                                <property name="release_status_list"/>
                                <property name="date_released"/>
                                <property name="owning_user"/>
                                <property name="last_mod_date"/>
                                <property name="last_mod_user"/>
                                <property name="Fnd0LogicalObjectTypeRel" />
                            </tableDisplay>
                            <thumbnailDisplay/>
                            <treeDisplay>
                                <property name="object_string"/>
                                <property name="object_type"/>
                                <property name="checked_out" renderingHint="label"/>
                                <property name="sr5DocLanguage"/>
                                <property name="release_status_list"/>
                                <property name="date_released"/>
                                <property name="owning_user"/>
                            </treeDisplay>
                            <listDisplay/>
                            <command actionKey="newBusinessObjectContextualAction" commandId="com.teamcenter.rac.common.AddNew" renderingHint="commandbutton"/>
                            <command commandId="Dma1Delete" />
                            <command actionKey="pasteAction" commandId="com.teamcenter.rac.viewer.pastewithContext" renderingHint="commandbutton"/>
                            <command actionKey="cutAction" commandId="org.eclipse.ui.edit.cut" renderingHint="commandbutton">
                                <parameter name="localSelection" value="true"/>
                            </command>
                        </objectSet>
                    </section>
                    <section titleKey="tc_xrt_Viewer">
                        <inject type="dataset" src="Awp0CrossSelectionViewer" module="js/aw-include.directive" />
                    </section>
                </column>
            </content>
        </page>
    </content>
</rendering>