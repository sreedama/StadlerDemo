<?xml version="1.0" encoding="UTF-8"?>
<!-- Default style sheet for displaying Bill Of Documents Node Revision Info Panel in Active Workspace Client. -->
<rendering>
    <header>
        <property name="object_string"/>
    </header>
    <page titleKey="tc_xrt_Summary">
        <section titleKey="tc_xrt_Preview">
            <image source="thumbnail"/>
        </section>
        <section titleKey="tc_xrt_properties">
            <property name="item_id" renderingHint="label"/>
            <property name="item_revision_id" renderingHint="label"/>
            <property name="object_name"/>
            <property name="object_desc"/>
            <property name="object_type"/>
            <separator/>
            <property name="owning_user" renderingHint="objectlink" modifiable="false"/>
            <property name="owning_group" renderingHint="objectlink" modifiable="false"/>
            <property name="creation_date" renderingHint="label"/>
            <property name="last_mod_date" renderingHint="label"/>
            <property name="last_mod_user" renderingHint="objectlink"/>
            <property name="checked_out_user" renderingHint="objectlink"/>
            <property name="checked_out_date" renderingHint="label"/>
        </section>
    </page>
</rendering>
