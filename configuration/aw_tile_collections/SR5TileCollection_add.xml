<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE ActiveWorkspaceGateway SYSTEM "Awp0aws2ActiveWorkspaceGateway.dtd" >
<ActiveWorkspaceGateway version="1.0">
    <!-- Tile Template definitions begins here -->

    <TileTemplate templateId="SR5CreateEngArticleTemplate">
        <ThemeIndex index="3" />
        <Icon>SR5EngArticle</Icon>
        <Action>Awp0ShowHomeFolder;cmdId=Awp0ShowCreateObject</Action>
        <ActionType type="3" />
    </TileTemplate>

    <TileTemplate templateId="SR5CreateDocumentTemplate">
        <ThemeIndex index="3"/>
        <Icon>Document</Icon>
        <Action>Awp0ShowHomeFolder;cmdId=Awp0ShowCreateObject</Action>
        <ActionType type="3"/>
    </TileTemplate>

    <TileTemplate templateId="SR5CreateArticleRequestTemplate">
        <ThemeIndex index="3" />
        <Icon>SR5ArticleRequestForm</Icon>
        <Action>Awp0ShowHomeFolder;cmdId=Awp0ShowCreateObject</Action>
        <ActionType type="3" />
    </TileTemplate>

    <TileTemplate templateId="SR5CreateChangeRequestTemplate">
        <ThemeIndex index="3" />
        <Icon>Change</Icon>
        <Action>Awp0GoChanges;cmdId=Cm1ShowCreateChange</Action>
        <ActionType type="3" />
    </TileTemplate>

    <!-- Tile definitions begins here -->
    <!-- Custom tiles -->

    <Tile tileId="SR5CreateEngArticleTile" templateId="SR5CreateEngArticleTemplate">
        <Name>Create Engineering Article</Name>
        <Parameter>cmdArg=SR5EngArticle</Parameter>
    </Tile>

    <Tile tileId="SR5CreateDocumentTile" templateId="SR5CreateDocumentTemplate">
        <Name>Create Document</Name>
        <Parameter>cmdArg=SR5TechDocument</Parameter>
    </Tile>

    <Tile tileId="SR5CreateArticleRequestTile" templateId="SR5CreateArticleRequestTemplate">
        <Name>Create CSP Request</Name>
        <Parameter>cmdArg=SR5ArticleReqForm</Parameter>
    </Tile>

    <Tile tileId="SR5CreateChangeRequestTile" templateId="SR5CreateChangeRequestTemplate">
        <Name>Create Change Request</Name>
        <Parameter>cmdArg=ChangeRequest</Parameter>
    </Tile>

    <!-- OOTB tiles -->

    <Tile tileId="Awp0HomeFolderTile" templateId="Awp0HomeFolderTemplate">
		<Name>Folders</Name>
	</Tile>

    <Tile tileId="Awp0InboxTile" templateId="Awp0InboxTemplate">
        <Name>Inbox</Name>
    </Tile>

    <Tile tileId="Awp0SearchTile" templateId="Awp0SearchTemplate">
        <Name>Saved Searches</Name>
    </Tile>

    <Tile tileId="Awp0MyActiveFoldersTile" templateId="Awp0MyActiveFoldersTemplate">
      <Name>My Active Folders</Name>
    </Tile>

    <Tile tileId="Awp0QuickStartTile" templateId="Awp0QuickStartTemplate">
        <Name>Help</Name>
    </Tile>

    <Tile tileId="Awp0FavoritesTile" templateId="Awp0FavoritesTemplate">
        <Name>Favorites</Name>
    </Tile>

    <Tile tileId="Awp0MyStuffTile" templateId="Awp0MyStuffTemplate">
        <Name>My Stuff</Name>
    </Tile>

    <Tile tileId="Cm1ChangesTile" templateId="Cm1ChangesTemplate">
        <Name>Changes</Name>
    </Tile>

    <Tile tileId="Mtw0MaterialSearchTile" templateId="Mtw0MaterialSearchTemplate">
        <Name>Material Search</Name>
    </Tile>

    <Tile tileId="Awp0ReportsTile" templateId="Awp0ReportsTemplate">
        <Name>Reports</Name>
    </Tile>

    <Tile tileId="Awp0ClassificationTile" templateId="Awp0ClassificationTemplate">
        <Name>Classification</Name>
    </Tile>

    <!-- Tile collection definitions begins here -->
    <TileCollection>
        <WorkspaceScope id="StadlerAuthor" />

        <!-- Group Main -->

        <CollectionTiles tileId="Awp0InboxTile" groupName="Main" size="1" ></CollectionTiles>
        <CollectionTiles tileId="Awp0HomeFolderTile" groupName="Main" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0SearchTile" groupName="Main" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0MyActiveFoldersTile" groupName="Main" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Cm1ChangesTile" groupName="Main" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Mtw0MaterialSearchTile" groupName="Main" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0ClassificationTile" groupName="Main" size="0" > </CollectionTiles>

        <!-- Group Quick Links -->

        <CollectionTiles tileId="SR5CreateEngArticleTile" groupName="Quick Links" size="0" ></CollectionTiles>
        <CollectionTiles tileId="SR5CreateDocumentTile" groupName="Quick Links" size="0" ></CollectionTiles>
        <CollectionTiles tileId="SR5CreateArticleRequestTile" groupName="Quick Links" size="0" ></CollectionTiles>
        <CollectionTiles tileId="SR5CreateChangeRequestTile" groupName="Quick Links" size="0" ></CollectionTiles>

        <!-- Group sr5misc -->

        <CollectionTiles tileId="Awp0FavoritesTile" groupName="Favorites" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0ReportsTile" groupName="Favorites" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0MyStuffTile" groupName="Favorites" size="0" ></CollectionTiles>
        <CollectionTiles tileId="Awp0QuickStartTile" groupName="Favorites" size="0" ></CollectionTiles>

    </TileCollection>
</ActiveWorkspaceGateway>